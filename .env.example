# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:8080/auth/google/callback

# Google APIs
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
GOOGLE_PLACES_API_KEY=your_google_places_api_key
GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key
GOOGLE_SPEECH_TO_TEXT_API_KEY=your_speech_to_text_api_key
GOOGLE_VISION_API_KEY=your_vision_api_key

# OpenAI
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo-preview

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# PostgreSQL
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=geulpi
POSTGRES_USER=geulpi_user
POSTGRES_PASSWORD=your_postgres_password

# JWT
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=7d

# E2E Test Mode
E2E_TEST_MODE=false
E2E_MOCK_EXTERNAL_APIS=true
E2E_AUTH_TOKEN=test_auth_token_for_e2e
E2E_USER_EMAIL=test@geulpi.com

# Kafka
KAFKA_BOOTSTRAP_SERVERS=kafka:9092
KAFKA_GROUP_ID=geulpi-group

# Application Ports
FRONTEND_PORT=3000
BACKEND_PORT=8080
ML_SERVER_PORT=8000